<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="15dp"
        android:height="15dp">
        <shape android:shape="oval">
            <size
                android:width="15dp"
                android:height="15dp" />
            <solid android:color="#E0AADC" />
        </shape>
    </item>
    <item
        android:width="5dp"
        android:height="5dp"
        android:gravity="center">
        <shape android:shape="oval">
            <size
                android:width="5dp"
                android:height="5dp" />
            <solid android:color="#50CDD4" />
        </shape>
    </item>
</layer-list>